{
  "name": "bower-files",
  "version": "1.1.1",
  "description": "Pulls in dynamic list of filepaths to bower components",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly",
    "codeclimate": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/.bin/codeclimate && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ksmithut/bower-files.git"
  },
  "keywords": [
    "bower",
    "task management"
  ],
  "author": "ksmithut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ksmithut/bower-files/issues"
  },
  "devDependencies": {
    "gulp": "^3.6.2",
    "gulp-exec": "^2.0.1",
    "gulp-istanbul": "^0.2.0",
    "gulp-jshint": "^1.6.1",
    "gulp-mocha": "^0.4.1",
    "gulp-util": "^2.2.14",
    "istanbul": "^0.2.10",
    "jshint-stylish": "^0.2.0",
    "mocha": "^1.18.2",
    "should": "^3.3.1",
    "mocha-lcov-reporter": "^0.0.1",
    "codeclimate-test-reporter": "^0.0.3"
  },
  "dependencies": {
    "glob": "^4.0.2",
    "lodash": "^2.4.1"
  }
}
